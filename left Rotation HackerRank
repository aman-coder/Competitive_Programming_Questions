// BY Brute force
public static List<Integer> rotateLeft(int d, List<Integer> arr) {
    // Write your code here
    int l=arr.size()-1;
    for(int i=0;i<d;i++){
        int temp = arr.get(0);
        for(int j=0;j<arr.size()-1;j++){
            arr.set(j,arr.get(j+1));
            
        }
        arr.set(l,temp);
    }
        return arr;
    }
    }
 //Optimal Solution
 
