class Solution {
    public boolean isPalindrome(int x) {
        int z =0, y=x;
        if(x<0){
            return false;
        }
        while(x !=0){
            int digit = x%10;
            z=z*10+digit;
            x/=10;
        }
        if(y == z )
        {
            return true;
        }else{
            return false;
        }
    }
}
